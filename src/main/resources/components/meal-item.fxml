      xmlns="http://javafx.com/javafx/11" xmlns:fx="http://javafx.com/fxml/1">
    <ImageView fitHeight="128" fitWidth="128" styleClass="mealImage" fx:id="mealImage"></ImageView>
    <Label styleClass="mealItemTitle" fx:id="mealItemTitle"></Label>
</VBox>
